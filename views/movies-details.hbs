<h1>{{movie.title}}</h1>
<hr>
<p>Year: {{movie.year}}</p>
<img src="{{movie.poster }}" alt="{{movie.title}}" />
<form action="/movie-details/:movieName/rating" method="POST" enctype="multipart/form-data">
  <label>Rating</label>
  <input type="number" name="rating" min="1" max="5" />
  <button type="submit">Submit</button>


</form>